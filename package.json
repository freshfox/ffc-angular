{
  "name": "ffc-angular",
  "version": "1.0.0-alpha.53",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ffc-angular -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc && karma start",
    "prepublishOnly": "npm run build",
    "release": "npm run prepublishOnly && npm publish dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freshfox/ffc-angular"
  },
  "author": {
    "name": "Alexander Ott",
    "email": "hello@freshfox.at"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freshfox/ffc-angular/issues"
  },
  "devDependencies": {
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@compodoc/compodoc": "^1.0.9",
    "@types/jasmine": "2.5.53",
    "@types/node": "^6.0.102",
    "angular-in-memory-web-api": "^0.3.2",
    "codelyzer": "~3.2.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.16.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "node-sass": "^4.8.3",
    "node-sass-tilde-importer": "^1.0.2",
    "node-watch": "^0.5.8",
    "protractor": "~5.1.2",
    "rollup": "^0.49.3",
    "run-sequence": "^1.2.2",
    "rxjs": "^6.0.0",
    "systemjs": "^0.20.12",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "zone.js": "^0.8.14"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/material": "^5.2.4",
    "@ngx-translate/core": "^9.1.1",
    "choices.js": "^3.0.3",
    "ladda": "^1.0.6",
    "numeral": "^2.0.6",
    "pikaday": "^1.7.0",
    "validator": "^9.4.1"
  }
}
