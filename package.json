{
  "name": "ffc-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn build:core && yarn build:auth && yarn build:firestore",
    "build:core": "ng build ff-ng-core && cp -r projects/freshfox/ng-core/src/styles ./dist/freshfox/ng-core && cp -r projects/freshfox/ng-core/src/can-deactivate-unsaved/i18n ./dist/freshfox/ng-core/can-deactivate-unsaved",
    "build:auth": "ng build ff-ng-auth && cp -r projects/freshfox/ng-auth/src/styles ./dist/freshfox/ng-auth && cp -r projects/freshfox/ng-auth/src/i18n ./dist/freshfox/ng-auth",
    "build:firestore": "ng build ff-ng-firestore",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release:core": "yarn publish dist/freshfox/ng-core --non-interactive",
    "release:auth": "yarn publish dist/freshfox/ng-auth --non-interactive",
    "release:firestore": "yarn publish dist/freshfox/ng-firestore --non-interactive",
    "release": "yarn release:core && yarn release:auth && yarn release:firestore"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/cdk": "^11.2.12",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/fire": "~6.1.3",
    "@angular/forms": "~12.0.0",
    "@angular/material": "^11.2.12",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "@ngx-translate/core": "^13.0.0",
    "debug": "^4.1.1",
    "firebase": "^7.24.0",
    "rxjs": "~6.5.4",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@angular/language-service": "~12.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}