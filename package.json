{
  "name": "ffc-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn build:core && yarn build:auth && yarn build:firestore",
    "build:core": "ng build ff-ng-core && cp -r projects/freshfox/ng-core/src/styles ./dist/freshfox/ng-core",
    "build:auth": "ng build ff-ng-auth && cp -r projects/freshfox/ng-auth/src/styles ./dist/freshfox/ng-auth",
    "build:firestore": "ng build ff-ng-firestore",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release:core": "yarn publish dist/freshfox/ng-core --non-interactive",
    "release:auth": "yarn publish dist/freshfox/ng-auth --non-interactive",
    "release:firestore": "yarn publish dist/freshfox/ng-firestore --non-interactive",
    "release": "yarn release:core && yarn release:auth && yarn release:firestore"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.2",
    "@angular/cdk": "^10.0.1",
    "@angular/common": "~10.0.2",
    "@angular/compiler": "~10.0.2",
    "@angular/core": "~10.0.2",
    "@angular/fire": "6.0.3",
    "@angular/forms": "~10.0.2",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "~10.0.2",
    "@angular/platform-browser-dynamic": "~10.0.2",
    "@angular/router": "~10.0.2",
    "@ngx-translate/core": "^13.0.0",
    "debug": "^4.1.1",
    "firebase": "^7.13.2",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "validator": "^11.1.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular-devkit/build-ng-packagr": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.2",
    "@angular/language-service": "~10.0.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
