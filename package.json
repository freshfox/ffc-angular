{
  "name": "ffc-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn build:core && yarn build:auth && yarn build:firestore",
    "build:core": "ng build ff-ng-core && cp -r projects/freshfox/ng-core/src/styles ./dist/freshfox/ng-core",
    "build:auth": "ng build ff-ng-auth && cp -r projects/freshfox/ng-auth/src/styles ./dist/freshfox/ng-auth",
    "build:firestore": "ng build ff-ng-firestore",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release:core": "yarn publish dist/freshfox/ng-core --non-interactive",
    "release:auth": "yarn publish dist/freshfox/ng-auth --non-interactive",
    "release:firestore": "yarn publish dist/freshfox/ng-firestore --non-interactive",
    "release": "yarn release:core && yarn release:auth && yarn release:firestore"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/fire": "^5.2.3",
    "@angular/forms": "~9.0.1",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@ngx-translate/core": "^12.1.1",
    "debug": "^4.1.1",
    "firebase": "^7.5.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "validator": "^11.1.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular-devkit/build-ng-packagr": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.7.5"
  }
}
